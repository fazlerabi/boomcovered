<InsuranceSvcRq>
  <RqUID>00000000-0000-0000-0000-000000000000</RqUID>
  <HomePolicyQuoteInqRq>
    <RqUID>00000000-0000-0000-0000-000000000000</RqUID>
    <TransactionRequestDt>{{effective_date}}</TransactionRequestDt>
    <CurCd>USD</CurCd>
    <InsuredOrPrincipal>
      <ItemIdInfo>
        <AgencyId>12345</AgencyId>
      </ItemIdInfo>
      <GeneralPartyInfo>
        <NameInfo>
          <PersonName>
            <Surname>{{lastname}}</Surname>
            <GivenName>{{firstname}}</GivenName>
          </PersonName>
        </NameInfo>
        <Addr>
          <AddrTypeCd>MailingAddress</AddrTypeCd>
          <Addr1>{{street}}</Addr1>
          <City>{{city}}</City>
          <StateProvCd>{{state}}</StateProvCd>
          <PostalCode>{{postal_code}}</PostalCode>
          <RegionCd>Region</RegionCd>
        </Addr>
        <Communications>
          <PhoneInfo>
            <CommunicationUseCd>Home</CommunicationUseCd>
            <PhoneNumber>+1-{{phone}}</PhoneNumber>
          </PhoneInfo>
          <EmailInfo>
            <EmailAddr>{{email}}</EmailAddr>
          </EmailInfo>
        </Communications>
      </GeneralPartyInfo>
      <InsuredOrPrincipalInfo>
        <InsuredOrPrincipalRoleCd>FNI</InsuredOrPrincipalRoleCd>
        <PersonInfo>
          <BirthDt>{{birthday}}</BirthDt>
        </PersonInfo>
      </InsuredOrPrincipalInfo>
    </InsuredOrPrincipal>
    <PersPolicy>
      <LOBCd>HOME</LOBCd>
      <ContractTerm> <!-- must be between now and today+1 month -->
        <EffectiveDt>{{effective_date}}</EffectiveDt> <!-- usually 1 year from EffectiveDt -->
        <ExpirationDt>{{expiration_date}}</ExpirationDt>
        <DurationPeriod>
          <NumUnits>12</NumUnits>
          <UnitMeasurementCd>MO</UnitMeasurementCd>
        </DurationPeriod>
      </ContractTerm>
      <PersApplicationInfo>
        <InsuredOrPrincipal/>
        <ResidenceOwnedRentedCd>OWNED</ResidenceOwnedRentedCd>
      </PersApplicationInfo>
    </PersPolicy>
    <Location id="LOC1">
      <ItemIdInfo>
        <AgencyId>12345</AgencyId>
      </ItemIdInfo>
      <Addr>
        <AddrTypeCd>PhysicalRisk</AddrTypeCd>
        <Addr1>{{street}}</Addr1>
        <City>{{city}}</City>
        <StateProvCd>{{state}}</StateProvCd>
        <PostalCode>{{postal_code}}</PostalCode>
        <RegionCd>Region</RegionCd>
      </Addr>
      {% if (mortgage and Boolean(mortgage) == true) %}
      <AdditionalInterest>
        <GeneralPartyInfo>
          <NameInfo>
            <CommlName>
              <CommercialName>{{mortgage_name}}</CommercialName>
            </CommlName>
          </NameInfo>
          <Addr>
            <Addr1>{{mortgage_street}}</Addr1>
            <City>{{mortgage_city}}</City>
            <StateProvCd>{{mortgage_state}}</StateProvCd>
            <PostalCode>{{mortgage_postal_code}}</PostalCode>
          </Addr>
        </GeneralPartyInfo>
      </AdditionalInterest>
      {% endif %}
    </Location>
    <HomeLineBusiness>
      <LOBCd>HOME</LOBCd>
      <Dwell LocationRef="LOC1" id="DWE1">
        <PolicyTypeCd>06</PolicyTypeCd>
        <PurchaseDt>{{purchaseDt}}</PurchaseDt>
        <Construction>
          <ConstructionCd>{{construction_type}}</ConstructionCd>
          <YearBuilt>{{year_built}}</YearBuilt>
          <FoundationCd>BS</FoundationCd>
          <NumStories>2</NumStories>
          <RoofingMaterial>
            <RoofMaterialCd>{{roof_type}}</RoofMaterialCd>
          </RoofingMaterial>
          <ElectricalPanelCd>CIRC</ElectricalPanelCd>
          <!-- <RoofGeometryTypeCd>{{roofShapeType}}</RoofGeometryTypeCd> -->
        </Construction>
        <!--<BldgImprovements>-->
        <!--<HeatingImprovementYear>2020</HeatingImprovementYear>-->
        <!--<PlumbingImprovementYear>2020</PlumbingImprovementYear>-->
        <!--<RoofingImprovementYear>2020</RoofingImprovementYear>-->
        <!--<WiringImprovementYear>2020</WiringImprovementYear>-->
        <!--</BldgImprovements>-->
        <DwellOccupancy>
          <YearOccupancy>{{yearOccupancy}}</YearOccupancy>
          <ResidenceTypeCd>CD</ResidenceTypeCd>
          <DwellUseCd>1</DwellUseCd>
          <OccupancyTypeCd>OWNER</OccupancyTypeCd>
          <com.stillwater_IsCorporationOwned>YES</com.stillwater_IsCorporationOwned>
          <com.stillwater_CorporationOwnerName>XYZ Inc.</com.stillwater_CorporationOwnerName>
          <com.stillwater_CorporationOwnerPhone>+1-540-555-1234</com.stillwater_CorporationOwnerPhone>
        </DwellOccupancy>
        <BldgProtection>
          <DistanceToFireStation>
            <NumUnits>3</NumUnits>
            <UnitMeasurementCd>MI</UnitMeasurementCd>
          </DistanceToFireStation>
          <DistanceToHydrant>
            <NumUnits>10</NumUnits>
            <UnitMeasurementCd>FT</UnitMeasurementCd>
          </DistanceToHydrant>
          <FireExtinguisherInd>1</FireExtinguisherInd>
          <ProtectionDeviceBurglarCd>CN</ProtectionDeviceBurglarCd>
          <ProtectionDeviceFireCd>CN</ProtectionDeviceFireCd>
          <ProtectionDeviceSmokeCd>NO</ProtectionDeviceSmokeCd>
          <ProtectionDeviceSprinklerCd>NO</ProtectionDeviceSprinklerCd>
          <DoorLockCd>DEADB</DoorLockCd>
        </BldgProtection>
        <BldgImprovements>
          <HeatingImprovementYear>2008</HeatingImprovementYear>
          <PlumbingImprovementYear>2010</PlumbingImprovementYear>
          <RoofingImprovementYear>2006</RoofingImprovementYear>
          <WiringImprovementYear>2010</WiringImprovementYear>
        </BldgImprovements>
        <DwellInspectionValuation>
          <NumFamilies>1</NumFamilies>
          <NumStoriesInDwellingCd>2</NumStoriesInDwellingCd>
          <BathroomInfo>
            <NumBathrooms>2</NumBathrooms>
            <ConstructionQualityCd>Basic</ConstructionQualityCd>
          </BathroomInfo>
          <FireplaceInfo>
            <NumFireplaces>1</NumFireplaces>
          </FireplaceInfo>
          <GarageInfo>
            <GarageTypeCd>Frame</GarageTypeCd>
            <AttachedInd>1</AttachedInd>
            <NumGarages>1</NumGarages>
          </GarageInfo>
          <KitchenInfo>
            <ConstructionQualityCd>Basic</ConstructionQualityCd>
          </KitchenInfo>
          <SubstructureInfo>
            <GroundFloorPct>1</GroundFloorPct>
            <FinishedBasementInfo>
              <FinishedBasementPct>0.5</FinishedBasementPct>
            </FinishedBasementInfo>
          </SubstructureInfo>
          <TotalLivingArea>
            <NumUnits>{{sqft}}</NumUnits>
          </TotalLivingArea>
        </DwellInspectionValuation>
        <Coverage>
          <CoverageCd>DWELL</CoverageCd>
          <Limit>
            <FormatInteger>{{dwell_coverage}}</FormatInteger>
          </Limit>
          <Limit>
            <FormatCurrencyAmt>
              <Amt>386000</Amt>
            </FormatCurrencyAmt>
          </Limit>
          <Deductible>
            <FormatInteger>1000</FormatInteger>
            <DeductibleAppliesToCd>AllPeril</DeductibleAppliesToCd>
          </Deductible>
        </Coverage>
        <Coverage>
          <CoverageCd>SEWER</CoverageCd>
          <Limit>
            <FormatInteger>10000</FormatInteger>
          </Limit>
        </Coverage>
        <Coverage>
          <CoverageCd>PP</CoverageCd>
          <Limit>
            <FormatInteger>35000</FormatInteger>
          </Limit>
          <Deductible>
            <FormatInteger>1000</FormatInteger>
            <DeductibleAppliesToCd>AllPeril</DeductibleAppliesToCd>
          </Deductible>
        </Coverage>
        <Coverage>
          <CoverageCd>LAC</CoverageCd>
          <Limit>
            <FormatInteger>10000</FormatInteger>
          </Limit>
        </Coverage>
        <Coverage>
          <CoverageCd>MEDPM</CoverageCd>
          <Limit>
            <FormatInteger>5000</FormatInteger>
          </Limit>
        </Coverage>
        <Coverage>
          <CoverageCd>PL</CoverageCd>
          <Limit>
            <FormatInteger>500000</FormatInteger>
          </Limit>
        </Coverage>
